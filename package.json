{
  "name": "ng2-boilerplate",
  "version": "0.0.1-alpha.1",
  "description": "An opinionated, experimental Angular 2 / Webpack boilerplate with tree-shaking.",
  "main": "src/main.browser.ts",
  "scripts": {
    "build": "rimraf dist && webpack --config build/webpack.config.js --progress --profile",
    "build:prod": "rimraf dist && webpack --config build/webpack.config.js --progress --profile",
    "clean": "rimraf dist node_modules && yarn cache clean",
    "clean:install": "yarn run clean && yarn install",
    "clean:start": "yarn run clean-install && yarn start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "pretest": "yarn run lint",
    "serve": "webpack-dev-server --config build/webpack.config.js --inline",
    "serve:prod": "webpack-dev-server --config build/webpack.config.js --inline",
    "start": "yarn run serve",
    "test": "karma start",
    "watch": "webpack --config build/webpack.config.js --watch --progress --profile",
    "watch:prod": "webpack --config build/webpack.config.js --watch --progress --profile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SeerUK/ng2-boilerplate.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "boilerplate",
    "ng",
    "ng2",
    "project",
    "template",
    "tree-shaking",
    "webpack"
  ],
  "author": "Elliot Wright <hello@elliotdwright.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SeerUK/ng2-boilerplate/issues"
  },
  "homepage": "https://github.com/SeerUK/ng2-boilerplate#readme",
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.46",
    "angular2-router-loader": "^0.3.3",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.5.1",
    "awesome-typescript-loader": "^2.2.4",
    "codelyzer": "^1.0.0-beta.3",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.1.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "2.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.10"
  }
}
